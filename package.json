    "html-minifier-terser": "^5.1.1"
  },
  "peerDependencies": {
    "vite": ">=2.0.0-beta.15"
  },
  "devDependencies": {
    "@types/ejs": "^3.0.5",
    "@types/html-minifier-terser": "^5.1.1",
    "@types/node": "^14.14.20",
    "conventional-changelog-cli": "^2.1.1",
    "prettier": "^2.2.1",
